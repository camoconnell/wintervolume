package com.camoconnell{		import flash.events.Event;		public class CursorEvent extends Event {				public static const CURSOR_EVENT:String = "CURSOR_EVENT";		public var cursorType:String;				public function CursorEvent( type:String, ct:String, bubbles:Boolean=true, cancelable:Boolean=false ){			this.cursorType = ct;			super(type, bubbles, cancelable);		}				override public function clone() : Event {			return new CursorEvent(type, cursorType, bubbles, cancelable);		}			}	}