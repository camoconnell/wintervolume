package com.camoconnell {    import com.camoconnell.CursorEvent;    import com.greensock.*;    import com.greensock.easing.*;    import flash.display.MovieClip;    import flash.events.*;    import flash.media.Sound;    public class Bomber extends MovieClip {        public function Bomber(): void {            init();        }        private function init(): void {}        public function removeListeners(): void {            removeEventListener(MouseEvent.ROLL_OUT, out);            removeEventListener(MouseEvent.ROLL_OVER, over);            removeEventListener(MouseEvent.CLICK, selected);        }        private function over(e: MouseEvent): void {            var bomberover: BomberOver = new BomberOver();            bomberover.play();            this.dispatchEvent(new CursorEvent(CursorEvent.CURSOR_EVENT, 'over'));            TweenLite.to(this, .2, {                frameLabel: "endFrame",                ease: Linear.easeNone            });        }        private function out(e: MouseEvent): void {            this.dispatchEvent(new CursorEvent(CursorEvent.CURSOR_EVENT, 'out'));            TweenLite.to(this, .8, {                frameLabel: "ambientFrame",                ease: Linear.easeNone            });        }        private function selected(e: MouseEvent): void {            this.removeListeners();            TweenLite.to(this, .2, {                frameLabel: "closingFrame",                ease: Linear.easeNone,                onComplete: finishSeq            });        }        private function finishSeq(): void {            var bleep: Bleep = new Bleep();            bleep.play();            TweenLite.to(this, .5, {                frameLabel: "startFrame",                delay: .2,                ease: Linear.easeNone,                onComplete: triggerEvent            });        }        private function triggerEvent(): void {            var explosion: Explosion = new Explosion();            explosion.play();            this.dispatchEvent(new BtnEvent(BtnEvent.BTN_EVENT));        }    }}