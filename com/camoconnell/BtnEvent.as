package com.camoconnell {    import flash.events.Event;     public class BtnEvent extends Event {         public static const BTN_EVENT:String = "BTN_EVENT";		public var data:String;         public function BtnEvent( type:String, d:String = "", bubbles:Boolean=true, cancelable:Boolean=false ){			this.data = d;        	super(type, bubbles, cancelable);        }         override public function clone() : Event {        	return new BtnEvent(type, data, bubbles, cancelable);        }     } }