package com.camoconnell.youth{	import com.greensock.TweenLite;	import com.greensock.easing.*;	import com.greensock.events.LoaderEvent;	import com.greensock.loading.SWFLoader;	import com.greensock.loading.display.ContentDisplay;		import com.camoconnell.CursorEvent;		import flash.display.*;	import flash.events.*;
		public class Item extends Sprite {				public var itemName:String;		public var itemTitle:String;		public var swfLoader:SWFLoader;		public var swf:MovieClip;		public var image:Sprite;		public var description:String;						public function Item(itemName:String, description:String, itemTitle:String, swfLoader:SWFLoader) {			this.itemName = itemName;			this.description = description;			this.swfLoader = swfLoader;		}				//The image is actually a Sprite that contains some description text at the bottom. If it has descriptive text, we put a 30% opaque black bar behind the white text to make it more readable too. 		public function attachClip():* {			//this.swf = MovieClip(swfLoader.rawContent);		//	this.addEventListener(MouseEvent.ROLL_OVER, _rollOverThumbnailHandler);		//	this.addEventListener(MouseEvent.ROLL_OUT, _rollOutThumbnailHandler);		//	this.addEventListener(MouseEvent.CLICK, _clickThumbnailHandler);			//this.swf.name = 'swf';			return swfLoader.rawContent; 						trace('swf')		}				public function dispose():void {			this.removeEventListener(MouseEvent.ROLL_OVER, _rollOverThumbnailHandler);			this.removeEventListener(MouseEvent.ROLL_OUT, _rollOutThumbnailHandler);			this.removeEventListener(MouseEvent.CLICK, _clickThumbnailHandler);			this.swfLoader.dispose(true);		}				private function _rollOverThumbnailHandler(event:MouseEvent):void {			this.dispatchEvent(new CursorEvent(CursorEvent.CURSOR_EVENT,'over'));		}				private function _rollOutThumbnailHandler(event:MouseEvent):void {			this.dispatchEvent(new CursorEvent(CursorEvent.CURSOR_EVENT,'out'));		}				private function _clickThumbnailHandler(event:MouseEvent):void {		}			}}