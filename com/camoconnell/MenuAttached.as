package com.camoconnell{	import com.greensock.*;	import com.greensock.plugins.*;		import flash.display.MovieClip;	import flash.events.*; 	public class MenuAttached extends MovieClip	{		private var isSelected:Boolean = false;		private var META_DATA:Array;		private var btnAr:Array;		public function MenuAttached(_META_DATA) : void		{			this.META_DATA = _META_DATA;			btnAr = [this.youth_mc,this.what_mc,this.who_mc,this.contact_mc,this.blog_mc,this.home_mc]			TweenPlugin.activate([TintPlugin]);			registerListeners();		}				public function registerListeners() : void		{			for(var i:int; i < btnAr.length; i++)			{				btnAr[i].stop();				btnAr[i].id = META_DATA[i].url;				btnAr[i].buttonMode = true;				btnAr[i].mouseChildren = false;				btnAr[i].hitArea = btnAr[i].hitArea_mc;				btnAr[i].addEventListener(MouseEvent.CLICK, handleMenuSelection, false, 0, true);				btnAr[i].addEventListener(MouseEvent.ROLL_OVER, handleOver, false, 0, true);			}		}				private function handleMenuSelection(e:MouseEvent) : void		{			this.dispatchEvent(new BtnEvent(BtnEvent.BTN_EVENT, e.target.id));			var flyby:Flyby = new Flyby();			flyby.play();		}				private function handleOver(e:MouseEvent) : void		{			this.dispatchEvent(new CursorEvent(CursorEvent.CURSOR_EVENT,'over'));			TweenLite.to(e.target, .5, {tint:0xffffff});			e.currentTarget.play();						e.currentTarget.addEventListener(MouseEvent.ROLL_OUT, handleOut, false, 0, true);		}				private function handleOut(e:MouseEvent) : void		{			this.dispatchEvent(new CursorEvent(CursorEvent.CURSOR_EVENT,'out'));			TweenLite.to(e.target, 1, {tint:null});			e.currentTarget.stop();						e.currentTarget.removeEventListener(MouseEvent.ROLL_OUT, handleOut);		}			} }